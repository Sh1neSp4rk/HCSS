components:
  schemas:
    CostAdjustmentRateSet:
      required:
        - businessUnitCode
        - rateSetGroupCode
      type: object
      properties:
        businessUnitCode:
          type: string
          description: Gets the business unit code.
        costAdjustmentRates:
          type: array
          items:
            $ref: '#/components/schemas/DisCostAdjustmentRate'
          description: Gets the cost adjustment rates.
          nullable: true
        effectiveDate:
          type: string
          description: Gets the effective date.
          format: date-time
          nullable: true
        rateSetGroupCode:
          type: string
          description: "Gets the rate set group code, used to uniquely identify this rate\r\nset."
        rateSetGroupDescription:
          type: string
          description: Gets the rate set group description.
          nullable: true
        id:
          type: string
          description: The Id of the rate.
          format: uuid
          nullable: true
      additionalProperties: false
    DisBusinessUnit:
      required:
        - code
        - id
      type: object
      properties:
        id:
          type: string
          description: The business unit id.
          format: uuid
        code:
          type: string
          description: A code, in all caps, that represents this business unit.
        description:
          type: string
          description: An optional description for this business unit.
          nullable: true
      additionalProperties: false
      description: "A business unit is a subdivision within a company that has separate data\r\nfrom other business units.  Business units are particularly useful for\r\nlarge companies that operate in many locations.  (e.g., each location\r\nmay be represented by a separate business unit for reporting and setup\r\npurposes)."
    DisCostAdjustmentRate:
      required:
        - costAdjustmentCode
        - rate
        - rateType
        - taxPercent
      type: object
      properties:
        costAdjustmentCode:
          type: string
          description: The cost adjustment code for which these rates should apply.
        rateType:
          $ref: '#/components/schemas/RateTimeType'
        rate:
          type: number
          description: The rate, usually in dollars per hour.
          format: double
        taxPercent:
          type: number
          description: Gets the tax percent.
          format: double
        id:
          type: string
          description: The Id of the rate.
          format: uuid
          nullable: true
      additionalProperties: false
    DisCostCode:
      required:
        - businessUnitCode
        - code
        - jobCode
      type: object
      properties:
        code:
          type: string
          description: Gets the cost code code.  This is a required field.
        businessUnitCode:
          type: string
          description: Gets the business unit.  This is a required field.
        jobCode:
          type: string
          description: Gets job code.  This is a required field.
        id:
          type: string
          description: The Id of the cost code.
          format: uuid
          nullable: true
        quantity:
          type: number
          description: The quantity.
          format: double
          nullable: true
        description:
          type: string
          description: The cost code description.
          nullable: true
        unitOfMeasurement:
          type: string
          description: The unit of measure.
          nullable: true
        laborHours:
          type: number
          description: The labor hours.
          format: double
          nullable: true
        equipmentHours:
          type: number
          description: The equipment hours.
          format: double
          nullable: true
        laborDollars:
          type: number
          description: The labor cost.
          format: double
          nullable: true
        equipmentDollars:
          type: number
          description: The equipment cost.
          format: double
          nullable: true
        materialDollars:
          type: number
          description: The material cost.
          format: double
          nullable: true
        subcontractDollars:
          type: number
          description: The subcontract cost.
          format: double
          nullable: true
        supplyDollars:
          type: number
          description: The supply cost.
          format: double
          nullable: true
        tags:
          type: array
          items:
            $ref: '#/components/schemas/DisTag'
          description: The cost code tags.
          nullable: true
        status:
          type: string
          description: 'The cost code status. Items Enum: "A" for Active, "C" for Completed, "I" for Inactive'
          nullable: true
        accountingCode:
          type: string
          description: The accounting code.
          nullable: true
        category:
          type: string
          description: '"Category" accounting field.'
          nullable: true
        criticalPathMethod:
          type: string
          description: '"CPM Code" accounting field.'
          nullable: true
        generalLedgerAccount:
          type: string
          description: '"GL Acct" accounting field.'
          nullable: true
        subJob:
          type: string
          description: '"Sub-Job" accounting field.'
          nullable: true
        workBreakdownStructureCode:
          type: string
          description: '"WBS Code" accounting field.'
          nullable: true
        note:
          type: string
          description: The cost code note.
          nullable: true
        estimateResources:
          type: string
          description: The estimate resources.
          nullable: true
        historyCode:
          type: string
          description: The historical activity code.
          nullable: true
        historicalBidItem:
          type: string
          description: The historical bid item.
          nullable: true
        heavyBidEstimateCode:
          type: string
          description: The HeavyBid estimate code.
          nullable: true
        isTM:
          type: boolean
          description: The flag indicating whether the cost code is a time-and-material item.
          nullable: true
        workType:
