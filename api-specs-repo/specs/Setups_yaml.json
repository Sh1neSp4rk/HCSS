openapi: 3.0.1
info:
  title: DIS API
  version: v1
  description: Integrate with accounting systems. Operations require the 'dis:read' or 'dis:write' scope.
  termsOfService: https://support.hcss.com.
  contact:
    name: Heavy Construction Systems Specialists, Inc. (HCSS)
    url: https://support.hcss.com/
    email: support@hcss.com
  license:
    name: EULA
    url: https://support.hcss.com.
servers:
  - url: https://api.hcssapps.com/setups
security:
  - bearer:
      - dis:read
      - dis:write
      - heavyjob:read
      - heavyjob:write
paths:
  /api/v1/AccountingTemplate:
    get:
      tags:
        - AccountingTemplate
      summary: Returns a list of accounting templates.
      description: "Returns a list of accounting templates for the specified business unit code.\r\n            \r\nFor help with authorization, please visit https://developer.hcssapps.com/getting-started/request-tokens"
      parameters:
        - name: businessUnitCode
          in: query
          description: The business unit code.
          required: true
          schema:
            type: string
          example: MANAGER
      responses:
        '200':
          description: Success
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/HeavyJobAccountingTemplate'
              example:
                - id: a55f2152-4eb5-4c62-88ea-223e4ab7c2e0
                  name: HWI-2876
                  isPrimary: false
        '400':
          description: Bad Request. See https://developer.hcssapps.com/getting-started/troubleshoot-bad-request for more information.
        '401':
          description: Unauthorized. See https://developer.hcssapps.com/getting-started/troubleshoot-unauthorized for more information.
        '403':
          description: Forbidden. See https://developer.hcssapps.com/getting-started/troubleshoot-forbidden for more information.
  /api/v1/CostCode/bulk:
    post:
      tags:
        - BulkCostCode
      summary: Creates multiple cost codes.
      description: "Creates multiple cost codes (up to 100), using its code as a unique identifier on the specified job and business unit.\r\n            \r\nFor help with authorization, please visit https://developer.hcssapps.com/getting-started/request-tokens"
      requestBody:
        description: The list of cost code.
        content:
          application/json:
            schema:
              maxItems: 100
              minItems: 1
              type: array
              items:
                $ref: '#/components/schemas/DisCostCode'
            example:
              - code: 8530000z
                businessUnitCode: manager
                jobCode: Z3924
                id: af8d7c49-35de-4266-b603-02f238f9fae1
                quantity: 1.5
                description: Purchase Pipe Materials
                unitOfMeasurement: LS
                laborHours: 0.5
                equipmentHours: 0.5
                laborDollars: 0.5
                equipmentDollars: 0.5
                materialDollars: 60311.92
                subcontractDollars: 0.5
                supplyDollars: 0.5
                tags:
                  - group: Cementation
                    tag: MIXING
                    tagDescription: Mixing concrete
                    id: ddd792d9-6bd0-4c5d-bfd0-04feae7376b1
                status: A
                accountingCode: 135z
                category: Category
                criticalPathMethod: CPM Code
                generalLedgerAccount: GL Acct
                subJob: Sub-Job
                workBreakdownStructureCode: WBS Code
                note: 12P - 413
                estimateResources: 01z
                historyCode: HistCode
                historicalBidItem: HistBidItem
                heavyBidEstimateCode: HBEstiMateCode
                isTM: false
                workType: G
                payItemNumber: PayItem1
                payItemFactor: 2.1
                isPayItemDriver: true
                accountingTemplateName: default
                isCapExpected: false
                isHiddenFromMobile: false
        required: true
      responses:
        '204':
          description: No Content
        '400':
          description: Bad Request. See https://developer.hcssapps.com/getting-started/troubleshoot-bad-request for more information.
        '401':
          description: Unauthorized. See https://developer.hcssapps.com/getting-started/troubleshoot-unauthorized for more information.
        '403':
          description: Forbidden. See https://developer.hcssapps.com/getting-started/troubleshoot-forbidden for more information.
  /api/v1/BusinessUnit:
    get:
      tags:
        - BusinessUnit
      summary: Returns a list of all business units.
      description: For help with authorization, please visit https://developer.hcssapps.com/getting-started/request-tokens
      responses:
        '200':
          description: Success
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/DisBusinessUnit'
              example:
                - id: 22fd9125-4d20-4bb3-9ea9-d3db41fa6293
                  code: manager
                  description: optional
        '400':
          description: Bad Request. See https://developer.hcssapps.com/getting-started/troubleshoot-bad-request for more information.
        '401':
          description: Unauthorized. See https://developer.hcssapps.com/getting-started/troubleshoot-unauthorized for more information.
        '403':
          description: Forbidden. See https://developer.hcssapps.com/getting-started/troubleshoot-forbidden for more information.
    post:
      tags:
        - BusinessUnit
      summary: Creates a new business unit.
      description: "Creates a new business unit, using the code as a unique identifier.\r\n\r\nFor help with authorization, please visit https://developer.hcssapps.com/getting-started/request-tokens"
      requestBody:
        description: The new business unit data.
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/SetupsBusinessUnitWrite'
            example:
              code: code
              description: optional description
        required: true
      responses:
        '200':
          description: Success
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/DisBusinessUnit'
              example:
                id: 22fd9125-4d20-4bb3-9ea9-d3db41fa6293
                code: manager
                description: optional
        '400':
          description: Bad Request. See https://developer.hcssapps.com/getting-started/troubleshoot-bad-request for more information.
        '401':
          description: Unauthorized. See https://developer.hcssapps.com/getting-started/troubleshoot-unauthorized for more information.
        '403':
          description: Forbidden. See https://developer.hcssapps.com/getting-started/troubleshoot-forbidden for more information.
  /api/v1/BusinessUnit/default:
    get:
      tags:
        - BusinessUnit
      summary: Returns the default business unit.
      description: For help with authorization, please visit https://developer.hcssapps.com/getting-started/request-tokens
      responses:
        '200':
          description: Success
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/DisBusinessUnit'
              example:
                id: 22fd9125-4d20-4bb3-9ea9-d3db41fa6293
                code: manager
                description: optional
        '400':
          description: Bad Request. See https://developer.hcssapps.com/getting-started/troubleshoot-bad-request for more information.
        '401':
          description: Unauthorized. See https://developer.hcssapps.com/getting-started/troubleshoot-unauthorized for more information.
        '403':
          description: Forbidden. See https://developer.hcssapps.com/getting-started/troubleshoot-forbidden for more information.
  /api/v1/CostCode:
    post:
      tags:
        - CostCode
      summary: Creates a new cost code.
      description: "Creates a new cost code, using its code as a unique identifier on the specified job and business unit.\r\n            \r\nFor help with authorization, please visit https://developer.hcssapps.com/getting-started/request-tokens"
      requestBody:
        description: The cost code.
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/DisCostCode'
            example:
              code: 8530000z
              businessUnitCode: manager
              jobCode: Z3924
              id: af8d7c49-35de-4266-b603-02f238f9fae1
              quantity: 1.5
              description: Purchase Pipe Materials
              unitOfMeasurement: LS
              laborHours: 0.5
              equipmentHours: 0.5
              laborDollars: 0.5
              equipmentDollars: 0.5
              materialDollars: 60311.92
              subcontractDollars: 0.5
              supplyDollars: 0.5
              tags:
                - group: Cementation
                  tag: MIXING
                  tagDescription: Mixing concrete
                  id: ddd792d9-6bd0-4c5d-bfd0-04feae7376b1
              status: A
              accountingCode: 135z
              category: Category
              criticalPathMethod: CPM Code
              generalLedgerAccount: GL Acct
              subJob: Sub-Job
              workBreakdownStructureCode: WBS Code
              note: 12P - 413
              estimateResources: 01z
              historyCode: HistCode
              historicalBidItem: HistBidItem
              heavyBidEstimateCode: HBEstiMateCode
              isTM: false
              workType: G
              payItemNumber: PayItem1
              payItemFactor: 2.1
              isPayItemDriver: true
              accountingTemplateName: default
              isCapExpected: false
              isHiddenFromMobile: false
      responses:
        '200':
          description: Success
        '400':
          description: Bad Request. See https://developer.hcssapps.com/getting-started/troubleshoot-bad-request for more information.
        '401':
          description: Unauthorized. See https://developer.hcssapps.com/getting-started/troubleshoot-unauthorized for more information.
        '403':
          description: Forbidden. See https://developer.hcssapps.com/getting-started/troubleshoot-forbidden for more information.
    get:
      tags:
        - CostCode
      summary: Returns a list of cost codes.
      description: "Returns a list of cost codes for the specified business unit code, job code, and accounting template.\r\n            \r\nFor help with authorization, please visit https://developer.hcssapps.com/getting-started/request-tokens"
      parameters:
        - name: businessUnitCode
          in: query
          description: The business unit code.
          required: true
          schema:
            type: string
          example: MANAGER
        - name: jobCode
          in: query
          description: The job code.
          required: true
          schema:
            type: string
          example: JOB123
        - name: accountingTemplateName
          in: query
          description: The accounting template name.  If blank or unspecified, the primary accounting template for the business unit is used.
          schema:
            type: string
            default: ''
          example: MAINTEMPLATE
      responses:
        '200':
          description: Success
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/DisCostCode'
              example:
                - code: 8530000z
                  businessUnitCode: manager
                  jobCode: Z3924
                  id: af8d7c49-35de-4266-b603-02f238f9fae1
                  quantity: 1.5
                  description: Purchase Pipe Materials
                  unitOfMeasurement: LS
                  laborHours: 0.5
                  equipmentHours: 0.5
                  laborDollars: 0.5
                  equipmentDollars: 0.5
                  materialDollars: 60311.92
                  subcontractDollars: 0.5
                  supplyDollars: 0.5
                  tags:
                    - group: Cementation
                      tag: MIXING
                      tagDescription: Mixing concrete
                      id: ddd792d9-6bd0-4c5d-bfd0-04feae7376b1
                  status: A
                  accountingCode: 135z
                  category: Category
                  criticalPathMethod: CPM Code
                  generalLedgerAccount: GL Acct
                  subJob: Sub-Job
                  workBreakdownStructureCode: WBS Code
                  note: 12P - 413
                  estimateResources: 01z
                  historyCode: HistCode
                  historicalBidItem: HistBidItem
                  heavyBidEstimateCode: HBEstiMateCode
                  isTM: false
                  workType: G
                  payItemNumber: PayItem1
                  payItemFactor: 2.1
                  isPayItemDriver: true
                  accountingTemplateName: default
                  isCapExpected: false
                  isHiddenFromMobile: false
        '400':
          description: Bad Request. See https://developer.hcssapps.com/getting-started/troubleshoot-bad-request for more information.
        '401':
          description: Unauthorized. See https://developer.hcssapps.com/getting-started/troubleshoot-unauthorized for more information.
        '403':
          description: Forbidden. See https://developer.hcssapps.com/getting-started/troubleshoot-forbidden for more information.
    patch:
      tags:
        - CostCode
      summary: Creates and updates cost codes.
      description: "This call also requires heavyjob:write scope.\r\nThis endpoint currently only supports the adding or updating of cost codes across \r\na single job. If more than one job ID is specified, the request will be rejected.\r\n            \r\nFor help with authorization, please visit https://developer.hcssapps.com/getting-started/request-tokens"
      requestBody:
        description: The list of cost codes.
        content:
          application/json-patch+json:
            schema:
              maxItems: 100
              minItems: 1
              type: array
              items:
                $ref: '#/components/schemas/DisCostCodeUpsert'
            example:
              - businessUnitCode: MANAGER
                jobCode: ROADPAVING
                code: COSTCODE1
                status: A
                description: First cost code.
                quantity: 1000.5
                unitOfMeasure: EA
                laborHours: 100.65
                equipmentHours: 45.2
                laborDollars: 321234.65
                equipmentDollars: 7656.2
                materialDollars: 6865.8
                subcontractDollars: 123.6
                supplyDollars: 21721.7
                note: cost code note.
                historicalActivityCode: 97HKG1
                isTm: true
                isHiddenFromMobile: false
                isCapExpected: true
          application/json:
            schema:
              maxItems: 100
              minItems: 1
              type: array
              items:
                $ref: '#/components/schemas/DisCostCodeUpsert'
            example:
              - businessUnitCode: MANAGER
                jobCode: ROADPAVING
                code: COSTCODE1
                status: A
                description: First cost code.
                quantity: 1000.5
                unitOfMeasure: EA
                laborHours: 100.65
                equipmentHours: 45.2
                laborDollars: 321234.65
                equipmentDollars: 7656.2
                materialDollars: 6865.8
                subcontractDollars: 123.6
                supplyDollars: 21721.7
                note: cost code note.
                historicalActivityCode: 97HKG1
                isTm: true
                isHiddenFromMobile: false
                isCapExpected: true
          text/json:
            schema:
              maxItems: 100
              minItems: 1
              type: array
              items:
                $ref: '#/components/schemas/DisCostCodeUpsert'
            example:
              - businessUnitCode: MANAGER
                jobCode: ROADPAVING
                code: COSTCODE1
                status: A
                description: First cost code.
                quantity: 1000.5
                unitOfMeasure: EA
                laborHours: 100.65
                equipmentHours: 45.2
                laborDollars: 321234.65
                equipmentDollars: 7656.2
                materialDollars: 6865.8
                subcontractDollars: 123.6
                supplyDollars: 21721.7
                note: cost code note.
                historicalActivityCode: 97HKG1
                isTm: true
                isHiddenFromMobile: false
                isCapExpected: true
          application/*+json:
            schema:
              maxItems: 100
              minItems: 1
              type: array
              items:
                $ref: '#/components/schemas/DisCostCodeUpsert'
            example:
              - businessUnitCode: MANAGER
                jobCode: ROADPAVING
                code: COSTCODE1
                status: A
                description: First cost code.
                quantity: 1000.5
                unitOfMeasure: EA
                laborHours: 100.65
                equipmentHours: 45.2
                laborDollars: 321234.65
                equipmentDollars: 7656.2
                materialDollars: 6865.8
                subcontractDollars: 123.6
                supplyDollars: 21721.7
                note: cost code note.
                historicalActivityCode: 97HKG1
                isTm: true
                isHiddenFromMobile: false
                isCapExpected: true
        required: true
      responses:
        '204':
          description: No Content
        '400':
          description: Bad Request. See https://developer.hcssapps.com/getting-started/troubleshoot-bad-request for more information.
        '401':
          description: Unauthorized. See https://developer.hcssapps.com/getting-started/troubleshoot-unauthorized for more information.
        '403':
          description: Forbidden. See https://developer.hcssapps.com/getting-started/troubleshoot-forbidden for more information.
  /api/v1/CostCode/{id}:
    put:
      tags:
        - CostCode
      summary: Updates an existing cost code by its id.
      description: "After the result of this operation, the server entity will match exactly what is sent here.  (i.e., excluded fields will be interpreted as \"not needed\" and deleted from the server)\r\n            \r\nFor help with authorization, please visit https://developer.hcssapps.com/getting-started/request-tokens"
      parameters:
        - name: id
          in: path
          description: The cost code guid.
          required: true
          schema:
            type: string
            format: uuid
      requestBody:
        description: The cost code.
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/DisCostCode'
            example:
              code: 8530000z
              businessUnitCode: manager
              jobCode: Z3924
