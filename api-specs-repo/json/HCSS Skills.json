    "paths": {
        "/v1/employeeSkills/employee/{employeeCode}": {
            "get": {
                "tags": [
                    "EmployeeSkills"
                ],
                "summary": "Returns all skills that the specified employee has.",
                "description": "Returns all skills that the specified employee has.\r\n\r\nA response header called \"Pagination-MorePagesAvailable\" will be sent in the response with a value of \"yes\" or \"no\", indicating whether more pages are available after\r\nthis one.  That is, if the header value is \"yes\", the client should request the next page.",
                "operationId": "EmployeeSkills_GetByEmployee",
                "consumes": [],
                "produces": [
                    "application/json",
                    "text/json"
                ],
                "parameters": [
                    {
                        "name": "employeeCode",
                        "in": "path",
                        "description": "The employee code",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "dateAfterUtc",
                        "in": "query",
                        "description": "Limits the results to employee skills that have been updated after the specified UTC time.",
                        "required": false,
                        "type": "string",
                        "format": "date-time"
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "The number of results returned.  Default 1000.",
                        "required": false,
                        "type": "integer",
                        "format": "int32"
                    },
                    {
                        "name": "offset",
                        "in": "query",
                        "description": "The page offset.  The response header \"Pagination-MorePagesAvailable\" indicates whether the call should be made again with an incremented offset.",
                        "required": false,
                        "type": "integer",
                        "format": "int32"
                    },
                    {
                        "name": "usePayrollCode",
                        "in": "query",
                        "description": "Specifies whether or not to include the HeavyJob payroll code for each employee in the response. The default value is false.",
                        "required": false,
                        "type": "boolean"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Ok",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/ApiEmployeeSkill"
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
