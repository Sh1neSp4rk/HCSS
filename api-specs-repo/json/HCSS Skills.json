    "definitions": {
        "ApiEmployeeSkill": {
            "required": [
                "EmployeeCode"
            ],
            "type": "object",
            "properties": {
                "EmployeeCode": {
                    "description": "A unique string identifier for this employee",
                    "type": "string"
                },
                "EmployeePayrollCode": {
                    "description": "The payroll code for the employee from HeavyJob. This value is only provided or used if the usePayrollCode\r\nURL parameter is specified.",
                    "type": "string"
                },
                "SkillName": {
                    "description": "The display name for the skill",
                    "type": "string"
                },
                "CourseCode": {
                    "description": "A unique string identifier for this skill's course code, can be the course name or course number in your LMS system, if this is not specified SkillName will be used unique identifier",
                    "type": "string"
                },
                "CertificationDate": {
                    "format": "date-time",
                    "description": "A datetime of when the employee was certified in this skill",
                    "type": "string"
                },
                "ExpirationDate": {
                    "format": "date-time",
                    "description": "A datetime of when this skill expires for this employee",
                    "type": "string"
                },
                "TrainingTime": {
                    "format": "int32",
                    "description": "TrainingTime is the amount of minutes it took the employee to complete the skill",
                    "type": "integer"
                },
                "Note": {
                    "description": "Note or description for this employee's skill",
                    "type": "string"
                },
                "IsDismissed": {
                    "description": "If the employee skill has been archived/dismissed if it is invalid, no longer in use, can be manually archived in Skills system",
                    "type": "boolean"
                },
                "LastModified": {
                    "format": "date-time",
                    "description": "Optional: when the employee skill was last modified",
                    "type": "string"
                },
                "CreatedDate": {
                    "format": "date-time",
                    "description": "Optional: when the employee skill was created",
                    "type": "string"
                },
                "Attachments": {
                    "description": "Attachments to the employees skill",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/ApiESAttachment"
                    }
                }
            }
        },
        "ApiESAttachment": {
            "description": "A POCO representing an arbitrary attachment on an employee skill.",
            "required": [
                "AttachmentUrl",
                "Name"
            ],
            "type": "object",
            "properties": {
                "AttachmentUrl": {
                    "description": "Url of the attachment",
                    "type": "string"
                },
                "Name": {
                    "description": "Display name of the attachment",
                    "type": "string"
                }
            }
        },
        "ApiSkill": {
            "description": "A POCO representing a skill from the skills API.",
            "required": [
                "Description",
                "HasExpiration",
                "SkillType"
            ],
            "type": "object",
            "properties": {
                "Name": {
                    "description": "Display name of the skill",
                    "type": "string"
                },
                "Description": {
                    "description": "Description of the skill",
                    "type": "string"
                },
                "HasExpiration": {
                    "description": "Boolean whether or not the skill expires",
                    "type": "boolean"
                },
                "ExpirationAlertThreshold": {
                    "format": "int32",
                    "description": "Threshold of days to the set Expiration when a skill changes to expiring",
                    "type": "integer"
                },
                "ExpiresAfter": {
                    "format": "int32",
                    "description": "Quantity of time units the skill expires after",
                    "type": "integer"
                },
                "ExpiresAfterUnits": {
                    "description": "Enum for time units Days = 1, Months = 2, Years = 3",
                    "enum": [
                        "Unknown",
                        "Days",
                        "Months",
                        "Years"
                    ],
                    "type": "string"
                },
                "TrainingTime": {
                    "format": "int32",
                    "description": "The amount of time in minutes the skill takes to complete, will be used in the Safety system as default training meeting duration if skill meetings are enabled",
                    "type": "integer"
                },
                "EnableTrainingMeetings": {
                    "description": "If training meetings for this skill are enabled (turns on the skill meeting integration)",
                    "type": "boolean"
                },
                "SkillType": {
                    "description": "A type to group skills by",
                    "type": "string"
                },
                "CourseCode": {
                    "description": "A unique string identifier for the course related to the skill, could be a course name or course number",
                    "type": "string"
                },
                "IsPrivate": {
                    "description": "Boolean to determine if skill is public or private",
                    "type": "boolean"
                },
                "LastModified": {
                    "format": "date-time",
                    "description": "When the skill was last modified",
                    "type": "string"
                },
                "CreatedDate": {
                    "format": "date-time",
                    "description": "When the skill was created",
                    "type": "string"
                },
                "Attachments": {
                    "description": "Attachments to the skill",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/ApiAttachment"
                    }
                }
            }
        },
        "ApiAttachment": {
            "description": "A POCO representing an arbitrary attachment on a skill.",
            "required": [
                "AttachmentUrl",
                "Name",
                "MimeType"
            ],
            "type": "object",
            "properties": {
                "AttachmentUrl": {
                    "description": "Url of the attachment",
                    "type": "string"
                },
                "Name": {
                    "description": "Display name of the attachment",
                    "type": "string"
                },
                "MimeType": {
                    "description": "MimeType examples: application/pdf image/png image/jpeg",
                    "type": "string"
                }
            }
        },
        "UpsertSkillResult": {
            "type": "object",
            "properties": {
                "SubmittedSkill": {
                    "$ref": "#/definitions/ApiSkill"
                },
                "Error": {
                    "type": "boolean"
                },
                "ErrorMessage": {
                    "type": "string"
                },
                "Code": {
                    "enum": [
                        "Continue",
                        "SwitchingProtocols",
                        "OK",
                        "Created",
                        "Accepted",
                        "NonAuthoritativeInformation",
                        "NoContent",
                        "ResetContent",
                        "PartialContent",
                        "MultipleChoices",
                        "Ambiguous",
                        "MovedPermanently",
                        "Moved",
                        "Found",
                        "Redirect",
                        "SeeOther",
                        "RedirectMethod",
                        "NotModified",
                        "UseProxy",
                        "Unused",
                        "TemporaryRedirect",
                        "RedirectKeepVerb",
                        "BadRequest",
                        "Unauthorized",
                        "PaymentRequired",
                        "Forbidden",
                        "NotFound",
                        "MethodNotAllowed",
                        "NotAcceptable",
                        "ProxyAuthenticationRequired",
                        "RequestTimeout",
                        "Conflict",
                        "Gone",
                        "LengthRequired",
                        "PreconditionFailed",
                        "RequestEntityTooLarge",
                        "RequestUriTooLong",
                        "UnsupportedMediaType",
                        "RequestedRangeNotSatisfiable",
                        "ExpectationFailed",
                        "UpgradeRequired",
                        "InternalServerError",
                        "NotImplemented",
                        "BadGateway",
                        "ServiceUnavailable",
                        "GatewayTimeout",
                        "HttpVersionNotSupported"
                    ],
                    "type": "string"
                },
                "SavedSkill": {
                    "$ref": "#/definitions/SK_Skill"
                },
                "SkillInserted": {
                    "type": "boolean"
                },
                "AttachmentsToCopyByUrl": {
                    "type": "object",
                    "additionalProperties": {
                        "$ref": "#/definitions/SF_Attachment"
                    }
                }
