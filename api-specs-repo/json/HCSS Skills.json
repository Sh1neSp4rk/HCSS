        "/v1/employeeSkills": {
            "get": {
                "tags": [
                    "EmployeeSkills"
                ],
                "summary": "Gets the cartesian product of employees and skills.",
                "description": "Returns a cross join of employees and skills, with each result representing a single skill for a single employee.  For example, if there are 10 employees and\r\neach employee has 10 skills, this call will return 100 results.\r\n\r\nA response header called \"Pagination-MorePagesAvailable\" will be sent in the response with a value of \"yes\" or \"no\", indicating whether more pages are available after\r\nthis one.  That is, if the header value is \"yes\", the client should request the next page.",
                "operationId": "EmployeeSkills_Get",
                "consumes": [],
                "produces": [
                    "application/json",
                    "text/json"
                ],
                "parameters": [
                    {
                        "name": "dateAfterUtc",
                        "in": "query",
                        "description": "Limits the results to employee skills that have been updated after the specified UTC time.",
                        "required": false,
                        "type": "string",
                        "format": "date-time"
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "The number of results returned.  Default 1000.",
                        "required": false,
                        "type": "integer",
                        "format": "int32"
                    },
                    {
                        "name": "offset",
                        "in": "query",
                        "description": "The page offset.  The response header \"Pagination-MorePagesAvailable\" indicates whether the call should be made again with an incremented offset.",
                        "required": false,
                        "type": "integer",
                        "format": "int32"
                    },
                    {
                        "name": "includeDismissed",
                        "in": "query",
                        "description": "Specifies whether or not to include dismissed skills in the response. The default value is false.",
                        "required": false,
                        "type": "boolean"
                    },
                    {
                        "name": "usePayrollCode",
                        "in": "query",
                        "description": "Specifies whether or not to include the HeavyJob payroll code for each employee in the response. The default value is false.",
                        "required": false,
                        "type": "boolean"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Ok",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/ApiEmployeeSkill"
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "EmployeeSkills"
                ],
                "summary": "Creates or updates a skill for an existing employee, using the skill's course code or name as a unique identifier.",
                "description": "Creates or updates a skill for an existing employee, using the skill's course code or name as a unique identifier.\r\n\r\nIf the skill does not exist, it is simply created.  If the skill already exists, the old skill is \"dismissed\" but retained\r\nfor record-keeping, and a new, non-dismissed skill is created.",
                "operationId": "EmployeeSkills_Post",
                "consumes": [
                    "application/json",
                    "text/json",
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "application/json",
                    "text/json"
                ],
                "parameters": [
                    {
                        "name": "skill",
                        "in": "body",
                        "description": "The skill to be upserted for an existing employee.",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/ApiEmployeeSkill"
                        }
                    },
                    {
                        "name": "usePayrollCode",
                        "in": "query",
                        "description": "Specifies whether or not to use the HeavyJob payroll code as the unique identifier for an employee. If true, EmployeePayrollCode is required and will be used instead of EmployeeCode to identify the employee that should receive the skill. The default value is false.",
                        "required": false,
                        "type": "boolean"
                    }
                ],
                "responses": {
                    "202": {
                        "description": "Accepted",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/v1/employeeSkills/import": {
            "post": {
                "tags": [
                    "EmployeeSkills"
                ],
                "summary": "Imports a list of employee skills, using the skill's course code or name as a unique identifier on each employee.",
                "description": "Imports a list of employee skills, using the skill's course code or name as a unique identifier on each employee.\r\n\r\nThis call does not create any new employees; all specified employee codes must belong to pre-existing employees.\r\n\r\nIf the skill does not exist for a particular employee, it is simply created.  If the skill already exists, the old\r\nskill is \"dismissed\" but retained for record-keeping, and a new, non-dismissed skill is created.",
                "operationId": "EmployeeSkills_Import",
                "consumes": [
                    "application/json",
                    "text/json",
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "application/json",
                    "text/json"
                ],
                "parameters": [
                    {
                        "name": "employeeSkills",
                        "in": "body",
                        "description": "A list of employee skills",
                        "required": true,
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/ApiEmployeeSkill"
                            }
                        }
                    },
                    {
                        "name": "usePayrollCode",
                        "in": "query",
                        "description": "Specifies whether or not to use the HeavyJob payroll code as the unique identifier for an employee. If true, EmployeePayrollCode is required and will be used instead of EmployeeCode to identify the employee that should receive the skill. The default value is false.",
                        "required": false,
                        "type": "boolean"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Ok",
                        "schema": {
                            "type": "object"
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
